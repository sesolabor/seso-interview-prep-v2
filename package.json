  "@comment testURL": "See: https://github.com/jsdom/jsdom/issues/2304",
  "scripts": {
    "dev": "nodemon --config nodemon.json server.ts false 3000",
    "postinstall": "npx next telemetry disable",
    "typecheck": "tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli --config repositories/ormconfig.ts",
    "worker": "ts-node -r tsconfig-paths/register workers",
    "db:init": "npm run db:init:dev && npm run db:migration:run",
    "db:init:dev": "docker exec seso-onsite-postgres psql -U postgres -c \"CREATE DATABASE seso_onsite\"",
    "db:migration:run": "node --max-old-space-size=2048 -- node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli --config repositories/ormconfig.ts migration:run"
  },
